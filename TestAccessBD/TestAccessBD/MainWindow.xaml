<Window x:Class="TestAccessBD.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:TestAccessBD"
        mc:Ignorable="d"
        Title="MainWindow" Height="531.908" Width="877.303">
    <Grid>
        <ListView Name="listView1" HorizontalAlignment="Left" Height="358" VerticalAlignment="Top" Width="808" Margin="30,92,0,0" Grid.ColumnSpan="2">
            <ListView.View>
                <GridView>
                    <GridViewColumn Width="100" Header="Asset ID" DisplayMemberBinding="{Binding AssetDBID}"></GridViewColumn>
                    <GridViewColumn Width="100" Header="Price" DisplayMemberBinding="{Binding EquityDB.price}"></GridViewColumn>
                </GridView>
            </ListView.View>
        </ListView>

    </Grid>
</Window>
